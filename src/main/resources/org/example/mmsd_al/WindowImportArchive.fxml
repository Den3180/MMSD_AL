<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane focusTraversable="true" 
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.mmsd_al.Windows.WindowImportArchive">
    <VBox alignment="CENTER" prefWidth="322.0" style="-fx-border-color: black; -fx-border-width: 1;"
          AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0"
          AnchorPane.rightAnchor="3" AnchorPane.topAnchor="3">
        <children>
            <GridPane alignment="CENTER">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="50.0" />
                </columnConstraints>
                <rowConstraints>
               <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50" vgrow="SOMETIMES" />
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <Label text="Доступно в архиве:" GridPane.rowIndex="1">
                        <font>
                            <Font size="14.0" />
                        </font>
                        <opaqueInsets>
                            <Insets />
                        </opaqueInsets>
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </GridPane.margin>
                    </Label>
                    <Label text="Загрузить:" GridPane.rowIndex="2">
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </GridPane.margin>
                        <font>
                            <Font size="14.0" />
                        </font>
                    </Label>
                    <Label text="Начало:" GridPane.rowIndex="3">
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </GridPane.margin>
                        <font>
                            <Font size="14.0" />
                        </font>
                    </Label>
               <TextField fx:id="availableRecords" editable="false" promptText="нет данных" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </GridPane.margin>
                  <font>
                     <Font size="14.0" />
                  </font>
               </TextField>
               <TextField fx:id="countLoadRecords" promptText="нет данных" GridPane.columnIndex="1" GridPane.rowIndex="2">
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </GridPane.margin>
                  <font>
                     <Font size="14.0" />
                  </font>
               </TextField>
               <TextField fx:id="startRecords" promptText="нет данных" GridPane.columnIndex="1" GridPane.rowIndex="3">
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </GridPane.margin>
                  <font>
                     <Font size="14.0" />
                  </font>
               </TextField>
               <Label layoutX="20.0" layoutY="80.0" text="Устройство:">
                  <font>
                     <Font size="14.0" />
                  </font>
                  <opaqueInsets>
                     <Insets />
                  </opaqueInsets>
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </GridPane.margin>
               </Label>
               <ComboBox fx:id="devArchiveComboBox" prefWidth="150.0" GridPane.columnIndex="1">
                  <GridPane.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </GridPane.margin>
               </ComboBox>
                </children>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            </GridPane>
         <HBox alignment="CENTER">
            <children>
                  <Button alignment="TOP_CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false"
                          onAction="#click_Button" prefHeight="30.0" prefWidth="91.0" text="Применить" textAlignment="CENTER">
                      <font>
                          <Font size="14.0" />
                      </font>
                  <HBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </HBox.margin>
                  </Button>
               <Button alignment="TOP_CENTER" cancelButton="true" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#click_Button" prefHeight="30.0" prefWidth="91.0" text="Отмена">
                  <font>
                     <Font size="14.0" />
                  </font>
                  <HBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </HBox.margin>
               </Button>
            </children>
         </HBox>
        </children>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </VBox>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
</AnchorPane>
